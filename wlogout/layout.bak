{
    "label" : "lock",
    "action" : "$HOME/binaries/utils/lock",
    "text" : "Lock (l)",
    "keybind" : "l"
}
{
    "label" : "logout",
    "action" : "kill -9 -1",
    "text" : "Logout (e)",
    "keybind" : "e"
}
{
    "label" : "shutdown",
    "action" : "systemctl poweroff",
    "text" : "Shutdown (s)",
    "keybind" : "s"
}
{
    "label" : "suspend",
    "action" : "systemctl suspend",
    "text" : "Suspend (u)",
    "keybind" : "u"
}
{
    "label" : "reboot",
    "action" : "systemctl reboot",
    "text" : "Reboot (r)",
    "keybind" : "r"
}
